{
  "manifest_version": 2,
  "name": "react-chrome-extension-starter",
  "author": "Devin Efendy",
  "version": "1.0.1",
  "description": "Starter Chrome Extension PopUp",
  "icons": {
    "16": "logo-128.jpeg",
    "48": "logo-128.jpeg",
    "128": "logo-128.jpeg"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": ["storage", "geolocation"]
}
